   "source": [
    "def getFactor(n):\n",
    "   return [factor for factor in range(1, n + 1) if n % factor == 0]\n",
    "# Example usage:\n",
    "number = 25\n",
    "factors = getFactor(number)\n",
    "print(f\"The factors of {number} are: {factors}\")\n"
   ]
