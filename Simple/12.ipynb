   "source": [
    "# Write a program to reverse a given number take input from user\n",
    "num = int(input(\"Enter a number: \"))\n",
    "reverse_num = 0\n",
    "while num > 0:\n",
    "    digit = num % 10\n",
    "    reverse_num = reverse_num * 10 + digit\n",
    "    num //= 10\n",
    "print(\"Reversed number:\", reverse_num)"
   ]
